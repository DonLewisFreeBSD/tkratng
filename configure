  withval=$with_tcl_include; tcl_with_include=$withval
fi

if test "x$tcl_with_include" != "x"; then
    if test "x$forced_tclsh" != "x"; then
       { { echo "$as_me:$LINENO: error: '--with-tcl-include can not be used together with --with-tclsh'" >&5
echo "$as_me: error: '--with-tcl-include can not be used together with --with-tclsh'" >&2;}
   { (exit 1); exit 1; }; }
    fi
    for i in $tcl_with_include ; do
	if test -r $i/tcl.h ; then
	    MAJOR=`grep TCL_MAJOR_VERSION $i/tcl.h | awk '{print $3}'`
	    MINOR=`grep TCL_MINOR_VERSION $i/tcl.h | awk '{print $3}'`
	    if test $MAJOR$MINOR -lt 83 ; then
		{ { echo "$as_me:$LINENO: error: 'Specified --with-tcl-include directory contains version $MAJOR.$MINOR. 8.3 or later required'" >&5
echo "$as_me: error: 'Specified --with-tcl-include directory contains version $MAJOR.$MINOR. 8.3 or later required'" >&2;}
   { (exit 1); exit 1; }; }
	    fi
	    required_tcl_version=$MAJOR.$MINOR
	fi
    done
fi

